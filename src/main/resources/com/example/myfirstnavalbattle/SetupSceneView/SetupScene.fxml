<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="@SetupSceneStyle.css"
            styleClass="anchorpane"
            prefHeight="400.0" prefWidth="600.0" fx:id="anchorpane"
            fx:controller="com.example.myfirstnavalbattle.controller.SetupController">
            <!--fx:controller="com.example.myfirstnavalbattle.SetupStageView.SetupStage"-->

    <GridPane styleClass="gridpane"
              fx:id="gridpane"
              gridLinesVisible="true"
              layoutX="200" layoutY="120">

    </GridPane>


    <HBox layoutY="200" layoutX="800"
          fx:id="hBox">
    </HBox>

    <Button layoutY="20" layoutX="20"
            styleClass="back-button"
            onAction="#handleBackButton">
    </Button>

    <Button layoutX="1000" layoutY="600"
            styleClass="ready-button"
            text="LISTO" onAction="#handleReadyButton"
            fx:id="readyButton">
    </Button>

</AnchorPane>
