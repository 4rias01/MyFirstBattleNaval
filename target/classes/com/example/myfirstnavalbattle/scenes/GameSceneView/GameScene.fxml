<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            stylesheets="@GameSceneStyle.css"
            styleClass="AnchorPane"
            fx:controller="com.example.myfirstnavalbattle.controller.GameController"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">

    <GridPane fx:id="gridPaneIA"
              layoutX="200" layoutY="120">
    </GridPane>

    <GridPane fx:id="gridPanePlayer"
              layoutX="800" layoutY="120" styleClass="GridPane">
    </GridPane>

    <Button layoutY="20" layoutX="20"
            styleClass="back-button"
            onAction="#handleBackButton">
    </Button>

</AnchorPane>
